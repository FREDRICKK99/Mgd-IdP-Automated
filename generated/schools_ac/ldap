    post-auth { update { description := "Authenticated at %S" } }

    options {
        chase_referrals = yes
        rebind = yes
        res_timeout = 10
        srv_timelimit = 3
        net_timeout = 1
        idle = 60
        probes = 3
        interval = 3
        ldap_debug = 0x0028
    }

    tls {}

    pool {
        start = ${thread[pool].start_servers} 
        min = ${thread[pool].min_spare_servers} 
        max = ${thread[pool].max_servers} 
        spare = ${thread[pool].max_spare_servers} 
        uses = 0
        retry_delay = 30
        lifetime = 0
        idle_timeout = 60
    }

}
